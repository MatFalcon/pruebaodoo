<?xml version="1.0"?>
<odoo>
    <record id="report_estate_property_offers" model="ir.actions.report">
        <field name="name">Estate Property Offers</field>
        <field name="model">estate.property</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_property_offers</field>
        <field name="report_file">estate.report_property_offers</field>
        <field name="print_report_name">'Estate Property Offers for - %s' % (object.name or 'Attendee')</field>
        <field name="binding_model_id" ref="model_estate_property"/>
        <field name="binding_type">report</field>
    </record>

    <record id="report_estate_property_users" model="ir.actions.report">
        <field name="name">Estate Property Users</field>
        <field name="model">res.users</field>
        <field name="report_type">qweb-pdf</field>
        <field name="report_name">estate.report_property_users</field>
        <field name="report_file">estate.report_property_users</field>
        <field name="print_report_name">'Estate Property Offers of - %s' % (object.name or 'Attendee')</field>
        <field name="binding_model_id" ref="base.model_res_users"/>
        <field name="binding_type">report</field>
    </record>
</odoo>
