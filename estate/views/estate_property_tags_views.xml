<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="action_estate_property_tags" model="ir.actions.act_window">
        <field name="name">estate property tags</field>
        <field name="res_model">estate.property.tags</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="estate_property_tags_view_tree" model="ir.ui.view">
        <field name="name">estate_property_tags_tree</field>
        <field name="model">estate.property.tags</field>
        <field name="arch" type="xml">
            <tree string="channel" editable="bottom">
                <field name="name"/>
            </tree>
        </field>
    </record>
    <record id="estate_property_tags_view_form" model="ir.ui.view">
        <field name="name">estate.property.tags.form</field>
        <field name="model">estate.property.tags</field>
        <field name="arch" type="xml">
            <form string="estate property">
                <sheet>
                    <field name="name"/>
                </sheet>
            </form>
        </field>
    </record>
</odoo>
