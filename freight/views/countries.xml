<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record id="action_countries" model="ir.actions.act_window">
        <field name="name">countries</field>
        <field name="res_model">freight.data</field>
        <field name="view_mode">tree,form</field>
    </record>
    <record id="view_countries_tree" model="ir.ui.view">
        <field name="name">countries.tree</field>
        <field name="model">freight.data</field>
        <field name="arch" type="xml">
            <tree>
                <field name="name"/>
                <!-- <field name="country_id"/> -->
            </tree>
        </field>
    </record>

    <record id="view_countries_form" model="ir.ui.view">
        <field name="name">countires.form</field>
        <field name="model">freight.data</field>
        <field name="arch" type="xml">
            <form>
                <sheet>
                    <group>
                        <field name="name"/>
                        <!-- <field name="country_id"/> -->
                    </group>
                </sheet>
            </form>
        </field>
    </record>
</odoo>