<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="awesome_dashboard.GalleryRenderer">
      
        <div class="d-flex p-2 flex-wrap">

            <t t-foreach="props.state.images" t-as="image" t-key="image.id">
                <div class="ratio ratio-1x1 m-2" style="width: 150px;">  
                    <img  t-att="{src: getImageUrl(image.id)}" />
                </div>
            </t>
        </div>
   
    </t>
</templates>
