<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="awesome_dashboard.GalleryImage">
        <div class="ratio ratio-1x1 m-2" style="width: 150px;">  
            <img  t-att="{src: getImageUrl(props.image.id)}" t-on-click="openForm" t-att-data-tooltip="this.props.image[props.state.archInfo.tooltip_field]" />
        </div>
    </t>
</templates>
