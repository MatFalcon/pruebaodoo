<?xml version="1.0" encoding="UTF-8"?>
<templates xml:space="preserve">
    <t t-name="awesome_owl.todo_item">
        <div t-att-class="{'text-muted': props.todo.isCompleted, 'text-decoration-line-through': props.todo.isCompleted}">


            <input type="checkbox" t-att-checked="props.todo.isCompleted" t-on-click="checkBoxClicked"/>
            <t t-esc="props.todo.description"/>
            <span class="fa fa-remove ms-4" t-on-click="deleteClicked"/>


        </div>

    </t>
</templates>
